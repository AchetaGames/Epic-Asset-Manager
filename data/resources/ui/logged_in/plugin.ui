<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="EpicPlugin" parent="GtkBox">
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkBox">
                <property name="spacing">5</property>
                <child>
                    <object class="GtkLabel">
                        <property name="can-focus">0</property>
                        <property name="label" translatable="yes" bind-source="EpicPlugin" bind-property="label" bind-flags="sync-create"/>
                        <property name="tooltip-text" translatable="yes" bind-source="EpicPlugin" bind-property="path" bind-flags="sync-create"/>
                        <property name="use-markup">true</property>
                        <property name="hexpand">True</property>
                        <property name="ellipsize">middle</property>
                        <property name="halign">start</property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
